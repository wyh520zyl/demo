<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_page"
    android:orientation="vertical"
    tools:ignore="ResourceName">

    <FrameLayout
        android:id="@+id/set_headline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">
        <FrameLayout
            android:id="@+id/personal_set_return"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="7.5dp"
            android:layout_marginStart="5dp"
            android:layout_marginBottom="4.5dp"
            android:onClick="personalSetOnclick"
            android:padding="10dp">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@mipmap/wuge_return"
                tools:ignore="ContentDescription" />
        </FrameLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_gravity="center"
            android:layout_marginTop="2.5dp"
            android:text="@string/discover_item_part_details"
            android:textSize="15sp"/>
    </FrameLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/set_headline">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="15dp"
        android:orientation="vertical"
        android:background="@drawable/wuge_banner"
        android:layout_marginRight="15dp">
        <RelativeLayout
            android:id="@+id/set_head"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="personalSetOnclick">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/personal_head"
                android:textSize="15sp"
                android:textStyle="bold"
                android:layout_marginStart="14.5dp"
                android:layout_marginTop="17dp"
                android:layout_marginBottom="17dp"
                android:textColor="@color/black"
                tools:ignore="RelativeOverlap" />
            <wuge.social.com.view.CircleImageView
                android:id="@+id/personal_set_heading"
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:layout_centerVertical="true"
                android:layout_toStartOf="@+id/personal_image"
                android:layout_marginEnd="12dp"
                android:onClick="personalSetOnclick"
                />
            <ImageView
                android:id="@+id/personal_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@mipmap/leftarrow_arrows"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="15dp"
                tools:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/information_shade"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:alpha="0"
                android:background="@drawable/background_view"
                tools:ignore="ContentDescription" />
        </RelativeLayout>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/personal_set_separate"
            tools:ignore="ContentDescription" />
        <RelativeLayout
            android:id="@+id/personal_set_nickname_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="personalSetOnclick">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/personal_nickname"
                android:textSize="15sp"
                android:textStyle="bold"
                android:layout_marginStart="14.5dp"
                android:layout_marginTop="17dp"
                android:layout_marginBottom="17dp"
                android:textColor="@color/black"/>
            <TextView
                android:id="@+id/personal_set_nickname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:textColor="@color/personal_essential_information"
                android:textSize="15sp"
                android:layout_toStartOf="@+id/personal_nickname"
                android:layout_marginEnd="12dp"
                tools:ignore="RelativeOverlap" />
            <ImageView
                android:id="@+id/personal_nickname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@mipmap/leftarrow_arrows"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="15dp"
                tools:ignore="ContentDescription" />
        </RelativeLayout>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/personal_set_separate"
            tools:ignore="ContentDescription" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/personal_id"
                android:textSize="15sp"
                android:textStyle="bold"
                android:layout_marginStart="14.5dp"
                android:layout_marginTop="17dp"
                android:layout_marginBottom="17dp"
                android:textColor="@color/black"/>
            <TextView
                android:id="@+id/personal_set_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:textColor="@color/personal_essential_information"
                android:textSize="15sp"
                android:layout_marginEnd="35dp"
                tools:ignore="RelativeOverlap" />
        </RelativeLayout>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/personal_set_separate"
            tools:ignore="ContentDescription" />
        <RelativeLayout
            android:id="@+id/personal_set_qr_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="personalSetOnclick">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/personal_qr_business_card"
                android:textSize="15sp"
                android:textStyle="bold"
                android:layout_marginStart="14.5dp"
                android:layout_marginTop="17dp"
                android:layout_marginBottom="17dp"
                android:textColor="@color/black"
                tools:ignore="RelativeOverlap" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:background="@mipmap/wuge_qr"
                android:layout_centerVertical="true"
                android:layout_marginEnd="35dp"
                tools:ignore="ContentDescription" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/personal_set_separate"
                tools:ignore="ContentDescription" />
        </RelativeLayout>

    </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="15dp"
            android:orientation="vertical"
            android:background="@drawable/wuge_banner"
            android:layout_marginRight="15dp">
            <RelativeLayout
                android:id="@+id/personal_set_gender_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="personalSetOnclick">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/personal_sex"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_marginStart="14.5dp"
                    android:layout_marginTop="17dp"
                    android:layout_marginBottom="17dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/personal_set_gender"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textColor="@color/personal_essential_information"
                    android:textSize="15sp"
                    android:layout_toStartOf="@+id/personal_sex"
                    android:layout_marginEnd="12dp"
                    tools:ignore="RelativeOverlap" />
                <ImageView
                    android:id="@+id/personal_sex"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@mipmap/leftarrow_arrows"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="15dp"
                    tools:ignore="ContentDescription" />
            </RelativeLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/personal_set_separate"
                tools:ignore="ContentDescription" />
            <RelativeLayout
                android:id="@+id/personal_set_birthday_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="personalSetOnclick">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/personal_birthday"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_marginStart="14.5dp"
                    android:layout_marginTop="17dp"
                    android:layout_marginBottom="17dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/personal_set_birthday"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textColor="@color/personal_essential_information"
                    android:textSize="15sp"
                    android:layout_toStartOf="@+id/personal_birth"
                    android:layout_marginEnd="12dp"
                    tools:ignore="RelativeOverlap" />
                <ImageView
                    android:id="@+id/personal_birth"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@mipmap/leftarrow_arrows"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="15dp"
                    tools:ignore="ContentDescription" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/personal_constellation"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_marginStart="14.5dp"
                    android:layout_marginTop="17dp"
                    android:layout_marginBottom="17dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/personal_set_constellation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:textColor="@color/personal_essential_information"
                    android:textSize="15sp"
                    android:layout_marginEnd="35dp"
                    tools:ignore="RelativeOverlap" />
            </RelativeLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/personal_set_separate"
                tools:ignore="ContentDescription" />
            <RelativeLayout
                android:id="@+id/personal_set_address_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="personalSetOnclick">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/personal_region"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_marginStart="14.5dp"
                    android:layout_marginTop="17dp"
                    android:layout_marginBottom="17dp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/personal_set_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="12dp"
                    android:layout_alignParentEnd="true"
                    android:textColor="@color/personal_essential_information"
                    android:textSize="15sp"
                    tools:ignore="RelativeOverlap" />
<!--                <ImageView-->
<!--                    android:id="@+id/personal_region"-->
<!--                    android:layout_width="wrap_content"-->
<!--                    android:layout_height="wrap_content"-->
<!--                    android:background="@mipmap/leftarrow_arrows"-->
<!--                    android:layout_alignParentEnd="true"-->
<!--                    android:layout_centerVertical="true"-->
<!--                    android:layout_marginEnd="15dp"-->
<!--                    tools:ignore="ContentDescription" />-->
            </RelativeLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/personal_set_separate"
                tools:ignore="ContentDescription" />
            <RelativeLayout
                android:id="@+id/personal_set_signature_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="personalSetOnclick">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/personal_personalized_signature"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_marginStart="14.5dp"
                    android:layout_marginTop="17dp"
                    android:layout_marginBottom="17dp"
                    android:textColor="@color/black"
                    tools:ignore="RelativeOverlap" />
                <TextView
                    android:id="@+id/personal_set_signature"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textColor="@color/personal_essential_information"
                    android:textSize="15sp"
                    android:gravity="right"
                    android:ellipsize="marquee"
                    android:layout_toStartOf="@+id/personal_personalized_signature"
                    android:layout_marginEnd="12dp"
                    android:singleLine="true"
                    tools:ignore="RtlHardcoded" />
                <ImageView
                    android:id="@+id/personal_personalized_signature"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@mipmap/leftarrow_arrows"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="15dp"
                    tools:ignore="ContentDescription" />
            </RelativeLayout>
        </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="15dp"
        android:orientation="vertical"
        android:background="@drawable/wuge_banner"
        android:layout_marginRight="15dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/my_class"
                android:textSize="15sp"
                android:textStyle="bold"
                android:layout_marginStart="14.5dp"
                android:layout_marginTop="17dp"
                android:layout_marginBottom="17dp"
                android:textColor="@color/black"/>
            <TextView
                android:id="@+id/personal_set_levels"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:textColor="@color/personal_essential_information"
                android:textSize="15sp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="35dp"
                tools:ignore="RelativeOverlap" />
        </RelativeLayout>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/personal_set_separate"
            tools:ignore="ContentDescription" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/my_charm"
                android:textSize="15sp"
                android:textStyle="bold"
                android:layout_marginStart="14.5dp"
                android:layout_marginTop="17dp"
                android:layout_marginBottom="17dp"
                android:textColor="@color/black"/>
            <TextView
                android:id="@+id/personal_set_charm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:textColor="@color/personal_essential_information"
                android:textSize="15sp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="35dp"
                tools:ignore="RelativeOverlap" />
        </RelativeLayout>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/personal_set_separate"
            tools:ignore="ContentDescription" />
        <RelativeLayout
            android:id="@+id/personal_set_value_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="personalSetOnclick">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/my_social"
                android:textSize="15sp"
                android:textStyle="bold"
                android:layout_marginStart="14.5dp"
                android:layout_marginTop="17dp"
                android:layout_marginBottom="17dp"
                android:textColor="@color/black"/>

            <TextView
                android:id="@+id/personal_set_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:textColor="@color/personal_essential_information"
                android:textSize="15sp"
                android:layout_toStartOf="@+id/personal_social"
                android:layout_marginEnd="12dp"
                tools:ignore="RelativeOverlap" />
            <ImageView
                android:id="@+id/personal_social"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@mipmap/leftarrow_arrows"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="15dp"
                tools:ignore="ContentDescription" />
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>
    </ScrollView>
    <ImageView
        android:id="@+id/personal_shade"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0"
        android:background="@drawable/background_view"
        tools:ignore="ContentDescription" />
</RelativeLayout>
